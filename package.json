{
  "name": "pps-fe-vue-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "npm run serve:dev",
    "serve:dev": "vue-cli-service serve --open --mode serve-dev",
    "serve:prod": "vue-cli-service serve --open --mode serve-prod",
    "serve:mock": "vue-cli-service serve --open --mode serve-mock",
    "build": "npm run build:dev",
    "build:dev": "vue-cli-service build --mode build-dev",
    "build:release": "vue-cli-service build --mode build-release",
    "build:prod": "vue-cli-service build --mode build-prod",
    "lint": "vue-cli-service lint",
    "lint:all:eslint": "eslint --fix --ext .js,.vue .",
    "lint:all:stylelint": "stylelint --fix \"src/**/*.{vue,less}\"",
    "lint:all:markdownlint": "markdownlint \"*.md\"",
    "lint:all:prettier": "prettier --write --list-different --loglevel warn \"**/*.{js,json,css,less,vue,html,md}\"",
    "lint:all": "run-s lint:all:*",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:watch": "vue-cli-service test:unit --watch --notify --notifyMode change",
    "release": "node scripts/release.js",
    "new": "hygen new"
  },
  "gitHooks": {
    "pre-commit": "cross-env PRE_COMMIT=true lint-staged"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "debounce-promise": "^3.1.2",
    "element-ui": "^2.8.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.11",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "vue": "^2.6.10",
    "vue-meta": "^1.6.0",
    "vue-router": "^3.0.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@laomao800/webpack-archive-plugin": "^2.0.0",
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-plugin-unit-jest": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-lodash": "^3.3.4",
    "compression-webpack-plugin": "^2.0.0",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.2.0",
    "eslint-plugin-vue": "^5.2.2",
    "execa": "^1.0.0",
    "express": "^4.16.3",
    "express-delay": "^0.2.0",
    "hygen": "^4.0.7",
    "imagemin-lint-staged": "^0.3.0",
    "inquirer": "^6.2.0",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "lint-staged": "^8.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "markdownlint-cli": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "semver": "^5.5.1",
    "stylelint": "^9.5.0",
    "stylelint-config-css-modules": "^1.3.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-recess-order": "^2.0.0",
    "stylelint-config-standard": "^18.2.0",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10"
  }
}
